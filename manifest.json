{
  "manifest_version": 3,
  "name": "zap medium",
  "version": "1.0",
  "description": "removes medium articles from google search results because they are junk!",
  "host_permissions": ["*://*.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  }
}
